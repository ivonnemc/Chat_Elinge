<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/background_light">


    <Button
        android:text="Regresar"
        android:layout_width="123dp"
        android:layout_height="40dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginStart="31dp"
        android:layout_marginBottom="15dp"
        android:id="@+id/button9"
        android:textSize="24sp"
        android:textColor="@android:color/background_light"
        android:textAlignment="center"
        android:background="@android:color/holo_blue_light" />

    <Button
        android:text="Siguiente"
        android:layout_width="123dp"
        android:layout_height="40dp"
        android:layout_alignBottom="@+id/button9"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="30dp"
        android:id="@+id/button10"
        android:background="@android:color/holo_blue_light"
        android:textSize="24sp"
        android:textColor="@android:color/background_light"
        android:textAlignment="center" />

    <EditText
        android:layout_width="338dp"
        android:layout_height="30dp"
        android:textSize="24sp"
        android:inputType="textPersonName"
        android:text="Apellidos"
        android:ems="10"
        android:id="@+id/editText8"
        android:textColor="@android:color/darker_gray"
        android:textAlignment="center"
        android:background="?android:attr/colorPressedHighlight"
        android:layout_marginBottom="15dp"
        android:layout_above="@+id/multiAutoCompleteTextView2"
        android:layout_alignStart="@+id/editText7"
        android:layout_alignEnd="@+id/editText7" />

    <EditText
        android:layout_width="338dp"
        android:layout_height="30dp"
        android:textSize="24sp"
        android:inputType="textEmailAddress"
        android:ems="10"
        android:id="@+id/editText9"
        android:text="Correo"
        android:textColor="@android:color/darker_gray"
        android:textAlignment="center"
        android:background="?android:attr/colorPressedHighlight"
        android:layout_marginBottom="68dp"
        android:layout_above="@+id/button9"
        android:layout_alignStart="@+id/multiAutoCompleteTextView2"
        android:layout_alignEnd="@+id/multiAutoCompleteTextView2" />

    <EditText
        android:layout_width="338dp"
        android:layout_height="30dp"
        android:textSize="24sp"
        android:inputType="textPersonName"
        android:text="Nombre"
        android:ems="10"
        android:id="@+id/editText7"
        android:textAlignment="center"
        android:textColor="@android:color/darker_gray"
        android:accessibilityTraversalBefore="?android:attr/colorPressedHighlight"
        android:background="?android:attr/colorPressedHighlight"
        android:layout_marginBottom="19dp"
        android:layout_above="@+id/editText8"
        android:layout_alignStart="@+id/button9"
        android:layout_alignEnd="@+id/button10" />

    <MultiAutoCompleteTextView
        android:text="Carrera"
        android:layout_width="338dp"
        android:layout_height="30dp"
        android:textSize="24sp"
        android:id="@+id/multiAutoCompleteTextView2"
        android:textColor="@android:color/darker_gray"
        android:textAlignment="center"
        android:background="?android:attr/colorPressedHighlight"
        android:layout_marginBottom="13dp"
        android:layout_above="@+id/editText9"
        android:layout_alignStart="@+id/editText8"
        android:layout_alignEnd="@+id/editText8" />

    <ImageView
        android:layout_width="200dp"
        android:layout_height="150dp"
        app:srcCompat="@drawable/icono_profesor"
        android:id="@+id/imageView3"
        android:layout_above="@+id/editText7"
        android:layout_centerHorizontal="true" />

    <TextView
        android:text="Registro profesor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView7"
        android:textSize="28sp"
        android:textColor="@android:color/black"
        android:textStyle="normal|bold"
        android:textAlignment="center"
        android:layout_above="@+id/imageView3"
        android:layout_centerHorizontal="true" />
</RelativeLayout>